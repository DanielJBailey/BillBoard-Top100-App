
<div class="boards-container">
        <% @boards.each do |board| %>
            <%= link_to board_path(board), :index => 'go' do %>
                <div class="board">
                    <%= board.name %>
                </div>
            <% end %>
        <% end %>
        <%= link_to "Create Board", new_board_path, class: "create-board-link" %>
</div>
