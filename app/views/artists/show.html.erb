<h1 class="section-title"><%= @artist.name %></h1>

<table>
    <tr>
        <th>Album No.</th>
        <th>Album Name</th>
    </tr>
    <% @counter = 1 %>
    <% @artist.songs.each do |song|%>
        <tr>
            <td><%= @counter %></td>
            <td><%= song.name %></td>
        </tr>
        <% @counter += 1 %>
    <% end %>
</table>


<%= link_to "Back", board_path(@board), class:"body-button"%>
